class hi
  const HLF_NUMBERS 1
  const HLF_STRINGS 2
  const HLF_CALLS 4

  enum
  0   HL_NORMAL
      HL_COMMENT
      HL_MLCOMMENT
      HL_KEYWORD1
      HL_KEYWORD2
      HL_NUMBER
      HL_STRING
      HL_CALL
  end

  head oper proc iskw 2 1
  head oper proc hltocolor 1 1
end

class syntax
  prop name 8
  prop flags 8
  prop scomment 8
  prop mcomments 8
  prop mcommente 8

  prop kw1 8
  prop kw2 8

  head oper proc new 6 1
end
