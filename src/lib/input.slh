class input
  enum
  127 EK_BACKSPACE

  500 EK_ARROW_LEFT
      EK_ARROW_RIGHT
      EK_ARROW_UP
      EK_ARROW_DOWN
      EK_DEL
      EK_PAGE_UP
      EK_PAGE_DOWN
      EK_HOME
      EK_END
  end

  head oper proc readchar 0 1
  head oper proc processkey 1 0
end
